{
  "name": "@aios-squads/finhealth-squad",
  "version": "1.0.0",
  "description": "Intelligent Financial Module for Healthcare with AI - TISS/SUS Billing, Auditing, Reconciliation, and Cash Flow Management",
  "author": "Noxtec",
  "license": "PROPRIETARY",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "healthcare",
    "fintech",
    "billing",
    "tiss",
    "sus",
    "audit",
    "ai-agents",
    "hospital-finance",
    "supabase"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "example:billing": "ts-node examples/run-billing-agent.ts",
    "example:seed": "ts-node examples/seed-test-data.ts",
    "db:schema": "echo 'Run schema.sql in your Supabase SQL editor'",
    "scrape:ans": "ts-node scripts/scrapers/ans-scraper.ts",
    "scrape:datasus": "ts-node scripts/scrapers/datasus-scraper.ts",
    "scrape:cbhpm": "ts-node scripts/scrapers/cbhpm-scraper.ts",
    "sync:tuss": "ts-node scripts/data-sync/sync-tuss.ts",
    "sync:sigtap": "ts-node scripts/data-sync/sync-sigtap.ts",
    "sync:cbhpm": "ts-node scripts/data-sync/sync-cbhpm.ts",
    "sync:tiss-xsd": "ts-node scripts/data-sync/sync-tiss-xsd.ts",
    "sync:all": "ts-node scripts/data-sync/sync-tuss.ts && ts-node scripts/data-sync/sync-sigtap.ts && ts-node scripts/data-sync/sync-cbhpm.ts && ts-node scripts/data-sync/sync-tiss-xsd.ts",
    "seed": "ts-node scripts/data-sync/seed-db.ts",
    "lint": "eslint src/",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config tests/integration/vitest.integration.config.ts",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker build -t finhealth-squad .",
    "docker:run": "docker run --rm -i --env-file .env finhealth-squad",
    "ci": "npm run lint && npm run typecheck && npm run test:run && npm run build"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0",
    "date-fns": "^3.0.0",
    "decimal.js": "^10.4.0",
    "dotenv": "^16.3.0",
    "adm-zip": "^0.5.10",
    "csv-parse": "^5.5.0",
    "fast-xml-parser": "^4.3.0",
    "js-yaml": "^4.1.0",
    "openai": "^4.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/adm-zip": "^0.5.5",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.18"
  }
}
